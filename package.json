{
  "name": "midi-to-strudel-converter",
  "version": "1.0.0",
  "description": "Convert MIDI files to Strudel code - Chrome Extension",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "verify": "bash scripts/verify-extension.sh",
    "generate-icons": "python3 scripts/create-icons.py",
    "rebuild": "npm run generate-icons && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "midi",
    "strudel",
    "music",
    "converter",
    "chrome-extension"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@tonejs/midi": "^2.0.28"
  }
}
